+package com.androidDev.dockital.screens.postNFT
+
+import android.graphics.Bitmap
+import android.net.Uri
+import android.provider.MediaStore
+import androidx.activity.compose.rememberLauncherForActivityResult
+import androidx.activity.result.contract.ActivityResultContracts
+import androidx.compose.foundation.*
+import androidx.compose.foundation.gestures.Orientation
+import androidx.compose.foundation.gestures.scrollable
+import androidx.compose.foundation.layout.*
+import androidx.compose.foundation.shape.RoundedCornerShape
+import androidx.compose.material.*
+import androidx.compose.material.icons.Icons
+import androidx.compose.material.icons.filled.Clear
+import androidx.compose.runtime.Composable
+import androidx.compose.runtime.mutableStateOf
+import androidx.compose.runtime.remember
+import androidx.compose.ui.Alignment
+import androidx.compose.ui.Modifier
+import androidx.compose.ui.draw.blur
+import androidx.compose.ui.geometry.CornerRadius
+import androidx.compose.ui.graphics.Color
+import androidx.compose.ui.graphics.PathEffect
+import androidx.compose.ui.graphics.asImageBitmap
+import androidx.compose.ui.graphics.drawscope.Stroke
+import androidx.compose.ui.platform.LocalConfiguration
+import androidx.compose.ui.platform.LocalContext
+import androidx.compose.ui.res.painterResource
+import androidx.compose.ui.text.font.FontWeight
+import androidx.compose.ui.text.style.TextAlign
+import androidx.compose.ui.tooling.preview.Preview
+import androidx.compose.ui.unit.dp
+import androidx.compose.ui.unit.sp
+import com.androidDev.dockital.R
+
+@Preview(showBackground = true)
+@Composable
+fun MintPushPreview(){
+    MintPush()
+}
+
+@Composable
+fun MintPush(){
+    var nameFilled: Boolean = true
+    var descripFilled:Boolean = true
+    var configuration = LocalConfiguration.current
+    var imageUri = remember {
+        mutableStateOf<Uri?>(null)
+    }
+    var imageFilled: Boolean = Uri.EMPTY.equals(imageUri)
+    var context = LocalContext.current
+    var bitmap =  remember {
+        mutableStateOf<Bitmap?>(null)
+    }
+
+    var launcher = rememberLauncherForActivityResult(contract = ActivityResultContracts.GetContent()) {
+            uri: Uri? -> imageUri.value = uri
+    }
+    var descriptionRem = remember{
+        mutableStateOf<String>("")
+    }
+    var nameRem = remember {
+        mutableStateOf<String>("")
+    }
+    var scrollState = rememberScrollState()
+    var blurValue = 0.dp
+    Column (
+        modifier = Modifier
+            .background(Color(32, 15, 52))
+            .fillMaxSize()
+            .blur(blurValue)
+            .padding(15.dp)
+            .scrollable(state = scrollState, Orientation.Vertical)
+            ){
+//        add Logo if Filling Empty
+//        Image(painter = , contentDescription = )
+
+        Spacer(
+            modifier = Modifier.padding(top = 50.dp)
+        )
+        Text(
+            text = "Create New NFT",
+            textAlign = TextAlign.Center,
+            color = Color.White,
+            fontSize = 30.sp,
+            fontWeight = FontWeight.Bold,
+//            modifier = Modifier.
+        )
+        Spacer(
+            modifier = Modifier.padding(top = 15.dp)
+        )
+        Row {
+            Text(
+                text = "Image, Video, Audio, or 3D Model",
+                color = Color.White,
+                fontWeight = FontWeight.Bold,
+                fontSize = 16.sp
+            )
+            Text(
+                text = "*",
+                color = Color.Red,
+                fontSize = 15.sp
+
+            )
+        }
+        Text(
+            text = "File Type Supported PNG, JPG, GIF, SVG, MP4",
+            color = Color.White,
+            fontSize = 13.sp
+        )
+        var stroke = Stroke(width = 5f,
+            pathEffect = PathEffect.dashPathEffect(floatArrayOf(10f, 10f), 0f)
+        )
+
+        Spacer(
+            modifier = Modifier.padding(top = 15.dp)
+        )
+        Box(
+            modifier = Modifier
+                .size(
+                    width = (configuration.screenWidthDp.dp),
+                    height = ((configuration.screenHeightDp * 0.3).dp)
+                )
+            //                .padding(horizontal = (configuration.screenWidthDp * 0.1).dp)
+        ) {
+            if (!imageFilled) {
+                var bitImage : Bitmap = MediaStore.Images.Med(context.contentResolver, imageUri.value)
+                context.contentResolver.query(
+                    MediaStore.Images.Media.EXTERNAL_CONTENT_URI
+                )
+                if(!(imageUri.value == null)){
+                    Image(
+                        modifier = Modifier
+                            .fillMaxSize(),
+                        bitmap = bitImage.asImageBitmap(),
+                        contentDescription = " "
+                    )
+                }
+
+            }
+            else{
+                Canvas(
+                    modifier = Modifier
+                        .fillMaxSize()
+
+                ) {
+                    drawRoundRect(
+                        color = Color.Green,
+                        style = stroke,
+                        cornerRadius = CornerRadius(8.dp.toPx())
+                    )
+                }
+                Button(
+                    colors = ButtonDefaults.buttonColors(backgroundColor = Color.White.copy(alpha = 0.3f)),
+                    modifier = Modifier
+                        .fillMaxSize(),
+                    onClick = {
+                        launcher.launch("image/*")
+                    },
+                )
+                {
+                    Column(
+                        modifier = Modifier
+                            .fillMaxSize(),
+                        horizontalAlignment = Alignment.CenterHorizontally,
+                        verticalArrangement = Arrangement.Center
+
+                    ) {
+                        Image(
+                            painter = painterResource(id = R.drawable.plus100),
+                            contentDescription = "Plus Icon",
+                            modifier = Modifier
+                                .width(100.dp)
+                                .height(100.dp),
+                        )
+
+                        Text(
+                            text = "Click To Add Media",
+                            color = Color.White,
+                            fontSize = 15.sp
+                        )
+
+                    }
+
+                }
+            }
+        }
+        Spacer(
+            modifier = Modifier.padding(top = 15.dp)
+        )
+        Row {
+            Text(
+                text = "Name ",
+                color = Color.White,
+                fontSize = 15.sp
+            )
+            Text(
+                text = "*",
+                color = Color.Red,
+                fontSize = 15.sp,
+
+            )
+        }
+        Spacer(
+            modifier = Modifier.padding(top = 3.dp)
+        )
+        TextField(
+            value = nameRem.value,
+            onValueChange = {
+                nameRem.value = it
+            },
+            trailingIcon = {
+                Icon(
+                    Icons.Default.Clear,
+                    contentDescription = "clear text",
+                    modifier = Modifier
+                        .clickable {
+                            nameRem = mutableStateOf(" ")
+                        }
+                )
+            },
+            colors = TextFieldDefaults.textFieldColors(textColor = Color.White),
+            modifier = Modifier
+                .border(width = 2.dp, color = Color.Green, shape = RoundedCornerShape(8.dp))
+                .fillMaxWidth()
+        )
+        Spacer(
+            modifier = Modifier.padding(top = 15.dp)
+        )
+        Row {
+            Text(
+                text = "Description ",
+                color = Color.White,
+                fontSize = 15.sp
+            )
+            Text(
+                text = "*",
+                color = Color.Red,
+                fontSize = 15.sp
+            )
+        }
+        Spacer(
+            modifier = Modifier.padding(top = 3.dp)
+        )
+        TextField(
+            value = descriptionRem.value,
+            onValueChange = {
+                descriptionRem.value = it
+                descripFilled = true
+            },
+            colors = TextFieldDefaults.textFieldColors(textColor = Color.White),
+            modifier = Modifier
+                .border(width = 2.dp, color = Color.Green, shape = RoundedCornerShape(8.dp))
+                .fillMaxWidth()
+                .height(140.dp)
+
+        )
+
+        if(!descripFilled){
+            Spacer(
+                modifier = Modifier.padding(top = 15.dp)
+            )
+            Text(
+                text = " Fill All Fields",
+                color = Color.Red,
+                fontSize = 15.sp
+            )
+        }
+        Button(
+            onClick =  {
+                if(nameRem.value.toString().isEmpty()){
+                    nameFilled = false
+                }
+                if(descriptionRem.value.toString().isEmpty()){
+                    nameFilled = false
+                }
+            },
+            colors = ButtonDefaults.buttonColors(backgroundColor = Color(84,180,53)),
+            modifier = Modifier
+                .fillMaxWidth()
+
+        ){
+            Text(
+                text = "Mint",
+                color = Color.White,
+                textAlign = TextAlign.Center
+            )
+        }
+        Spacer(
+            modifier = Modifier.padding(top = 15.dp)
+        )
+        Row {
+            Text(
+                text = "*",
+                color = Color.Red,
+                fontSize = 15.sp
+            )
+            Text(
+                text = "Required Fields",
+                color = Color.White,
+                fontSize = 15.sp
+            )
+        }
+    }
+}
